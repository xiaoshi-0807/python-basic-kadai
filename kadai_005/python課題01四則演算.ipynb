        "#上辺：10cm\n",
        "#下辺：20cm\n",
        "#高さ：5cm\n",
        "\n",
        "#公式：台形の面積 =（上辺＋下辺）× 高さ ÷ 2\n",
        "\n",
        "top_edge = 10\n",
        "bottom_edge = 20\n",
        "high = 5\n",
        "area = (top_edge+bottom_edge)*high/2\n",
        "print(f\"{area}cm²\")\n"
      ]
